package com.cjm.game.ai.pathfinding{	import flash.display.Shape;	import com.cjm.game.ai.pathfinding.INode;		public class Node extends Shape implements INode 	{		private var _f:Number;		private var _g:Number;		private var _h:Number;		private var _walkable:Boolean = true;		private var _parentNode:INode;				public function Node()		{			super();				}				public function render():void		{			this.graphics.lineStyle(2, 0x000000, 1);			this.graphics.moveTo(-10,-10);			this.graphics.lineTo(10,-10);			this.graphics.lineTo(10,10);			this.graphics.lineTo(-10,10);			this.graphics.lineTo(-10,-10);		}				public function set f(n:Number):void		{			_f = n;		}		public function get f():Number		{			return _f;		}				public function set g(n:Number):void		{			_g = n;		}		public function get g():Number		{			return _g;		}				public function set h(n:Number):void		{			_h = n;		}		public function get h():Number		{			return _h;		}				public function set walkable(b:Boolean):void		{			_walkable = b;		}		public function get walkable():Boolean		{			return _walkable;		}				public function set parentNode(n:INode):void		{			_parentNode = n;		}		public function get parentNode():INode		{			return _parentNode;		}					}}