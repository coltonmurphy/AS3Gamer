package com.cjm.game.pathfinding{	import com.cjm.game.core.IRender;	import com.cjm.math.geom.Vector2D;	import flash.display.DisplayObject;	import flash.display.Shape;	import com.cjm.game.ai.pathfinding.INode;		public class Node extends Vector2D implements INode, IRender	{		private var _f:Number;		private var _g:Number;		private var _h:Number;		private var _parent:INode;		private var _visible:Boolean;						public function Node( x:Number, y:Number)		{			super(x, y);						_visible = false;		}				public function render(...payLoad):void		{			if ( _visible )			{				var parentView:DisplayObject = payLoad[0] as DisplayObject;				_view = new Shape();				_view.graphics.lineStyle(1, 1);				_view.graphics.beginFill(0xC4FB89);				_view.graphics.drawCircle(0, 0, 1);				_view.graphics.endFill();				_view.x = x;				_view.y = y;				parentView.addChild(_view);			}		}				public function set f(n:Number):void		{			_f = n;		}		public function get f():Number		{			return _f;		}				public function set g(n:Number):void		{			_g = n;		}		public function get g():Number		{			return _g;		}				public function set h(n:Number):void		{			_h = n;		}		public function get h():Number		{			return _h;		}		public function set parent(n:INode):void		{			_parentNode = n;		}		public function get parent():INode		{			return _parent;		}		}}