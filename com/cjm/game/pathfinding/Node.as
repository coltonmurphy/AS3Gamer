package com.cjm.game.pathfinding{	import com.cjm.game.core.IRender;	import flash.display.Shape;	import com.cjm.game.ai.pathfinding.INode;	import flash.geom.Vector3D;		public class Node extends Vector3D implements INode, IRender	{		private var _f:Number;		private var _g:Number;		private var _h:Number;		private var _walkable:Boolean = true;		private var _parent:INode;		private var _visible:Boolean;						public function Node()		{			super();				_walkable = true;		}				public function render(...payLoad):void		{			if ( super.visible )			{				_view = new Shape()							}					}				public function set f(n:Number):void		{			_f = n;		}		public function get f():Number		{			return _f;		}				public function set g(n:Number):void		{			_g = n;		}		public function get g():Number		{			return _g;		}				public function set h(n:Number):void		{			_h = n;		}		public function get h():Number		{			return _h;		}				public function set walkable(b:Boolean):void		{			_walkable = b;		}		public function get walkable():Boolean		{			return _walkable;		}				public function set parent(n:INode):void		{			_parentNode = n;		}		public function get parent():INode		{			return _parent;		}									}}