package com.cjm.graph{	import com.cjm.game.core.IRender;	import com.cjm.math.geom.Vector2D;	import flash.display.DisplayObject;	import flash.display.Shape;	import com.cjm.pathfinding.INode;		public class GraphNode implements INode	{		private var _index:int;		private var _position:Vector2D;		private var _id:String;// town name, waypoint id, ect		public function GraphNode( index:int, position:Vector2D, id:String = "No ID" )		{			_index = index;			_position = position;		}		public function getPosition():int 		{			return _position;		}				public function getIndex():int 		{			return _index;		}				public function getID():String		{			return _id;		}		public function equals(other:GraphNode):Boolean		{			return _index == other.index();		}	}}