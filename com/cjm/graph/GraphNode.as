package com.cjm.graph{	import com.cjm.game.core.IRender;	import com.cjm.math.geom.Vector2D;	import flash.display.DisplayObject;	import flash.display.Shape;	import com.cjm.game.ai.pathfinding.INode;		public class GraphNode implements INode	{		private var _index:int;		private var _position:Vector2D;				public function GraphNode( index:int, position:Vector2D )		{			_index = index;			_position = position;		}		public function getPosition():int 		{			return _position;		}				public function getIndex():int 		{			return _index;		}		public function equals(other:GraphNode):Boolean		{			return _index == other.index();		}	}}