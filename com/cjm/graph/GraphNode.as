package com.cjm.graph{		import com.cjm.math.geom.Vector2D;	import flash.display.DisplayObject;	import flash.display.Shape;	import com.cjm.graph.INode;		public class GraphNode implements INode	{		private var _index:int;		private var _position:Vector2D;		private var _id:String;// town name, waypoint id, ect		public function GraphNode( index:int, position:Vector2D, id:String = "No ID" )		{			_index = index;			_position = position;			_id = id;		}		public function getPosition():Vector2D 		{			return _position;		}				public function getIndex():int 		{			return _index;		}				public function getID():String		{			return _id;		}		public function equals(other:INode):Boolean		{			return _index == other.getIndex();		}	}}