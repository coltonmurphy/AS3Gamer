package formosus.io{	import flash.utils.Dictionary;
	import flash.utils.ByteArray;	
	public class FormoCache	{		private static var _instance:Dictionary = new Dictionary();				public function FormoCache(access:Private)		{			if (access == null) throw new Error("SingletonException");		}				public static function getInstance(key:String = "default"):FormoCache		{			if (_instance[key])			{				_instance[key] = new FormoCache(new Private());			}						return _instance[key] as FormoCache;		}				private var _cache:Object = {};				public function get(key:String):ByteArray 		{			return this._cache[key] as ByteArray;		}		public function add(key:String, bytes:ByteArray):void		{			this._cache[key] = bytes;			}				public function has(key:String):Boolean		{			return this._cache[key] != undefined;		}		public function clear(key:String = null):void		{			if(key)			{				delete this._cache[key];			}			else			{				for(var k:String in this._cache)				{					delete this._cache[k];				}			}		}	}}class Private{}